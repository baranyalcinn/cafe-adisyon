{
  "name": "caffio",
  "version": "1.0.0",
  "description": "Caffio - Modern Cafe Management System",
  "main": "./out/main/index.js",
  "author": "Baran",
  "homepage": "https://electron-vite.org",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint --cache .",
    "test": "vitest",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false --skipLibCheck",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npx prisma generate && npx prisma db push && npm run typecheck && set NODE_OPTIONS=--max-old-space-size=8192 && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && set NODE_OPTIONS=--max-old-space-size=8192 && electron-builder --dir",
    "build:win": "npm run build && set NODE_OPTIONS=--max-old-space-size=8192 && electron-builder --win --x64",
    "build:mac": "electron-vite build && electron-builder --mac",
    "build:linux": "electron-vite build && electron-builder --linux",
    "db:reset": "npx tsx scripts/reset-db.ts",
    "db:soft-reset": "npx tsx scripts/soft-reset.ts"
  },
  "dependencies": {
    "@libsql/client": "^0.17.0",
    "@prisma/adapter-libsql": "7.3.0",
    "@prisma/client": "7.3.0",
    "@tanstack/react-query": "^5.90.20",
    "@tanstack/react-query-devtools": "^5.91.2",
    "bufferutil": "^4.1.0",
    "detect-libc": "^2.0.2",
    "js-base64": "^3.7.7",
    "libsql": "^0.5.22",
    "react-error-boundary": "^6.1.0",
    "utf-8-validate": "^6.0.6"
  },
  "overrides": {
    "glob": "^10.4.0",
    "rimraf": "^6.0.1"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "^3.0.0",
    "@electron-toolkit/eslint-config-ts": "^3.1.0",
    "@electron-toolkit/preload": "^3.0.2",
    "@electron-toolkit/tsconfig": "^2.0.0",
    "@electron-toolkit/utils": "^4.0.0",
    "@electron/rebuild": "^4.0.2",
    "@neon-rs/load": "^0.1.82",
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@tailwindcss/vite": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/node": "^25.0.9",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "electron": "^40.0.0",
    "electron-builder": "26.4.0",
    "electron-vite": "^5.0.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "input-otp": "^1.4.2",
    "jsdom": "^27.4.0",
    "lucide-react": "^0.563.0",
    "prettier": "^3.7.4",
    "prisma": "7.3.0",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-is": "^19.2.3",
    "recharts": "^3.6.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vite": "^7.2.6",
    "vitest": "^4.0.18",
    "zod": "^4.3.5",
    "zustand": "^5.0.10"
  }
}
